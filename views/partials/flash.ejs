<% if(success && success.length) { %>
<div
  id="pagbtn"
  class="alert alert-success alert-dismissible fade show d-inline"
  role="alert"
>
  <%= success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> <% if(error && error.length) { %>
<div
  class="alert alert-danger alert-dismissible fade show d-inline"
  role="alert"
>
  <%= error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>
